        <activity android:name=".PhysicsAnimationActivity" />
        <activity android:name=".AnimationDrawableActivity" />
        <activity android:name=".RevealHideViewCrossfadeActivity" />
        <activity android:name=".RevealHideViewCardflipActivity" />
        <activity android:name=".MoveViewAnimationActivity" />
        <activity android:name=".ZoomAnimationActivity" />
        <activity android:name=".AutoAnimateLayoutActivity" />
        <activity android:name=".LayoutChangeWithTransitionActivity" />
        <activity android:name=".AnimationWithActivity1" />
        <activity android:name=".AnimationWithActivity2" />
